let kCat = ./../Category/Cat/semigroupal

let vObject = Type

in    λ(object : Kind)
    → λ(cat : ./../Category/Monoidal/Kind kCat vObject object)
    → λ(m : object)
    →   { unit : cat.arrow { _1 = cat.unit, _2 = m } }
      ⩓ ./../Semigroup/Type
          object
          (./../Category/Monoidal/extractSemigroupal kCat vObject object cat)
          m
