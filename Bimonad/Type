let kCat = ./../Category/Cat/semigroupal

let vObject = Type

in    λ(object : Kind)
    → λ(v : ./../Category/Kind kCat Type vObject)
    → λ(cat : ./../Category/Kind kCat vObject object)
    → ./../Bimonoid/Type
        (kCat.arrow object object)
        (./../Category/Endofunctor/monoidal/monadic object v cat)
