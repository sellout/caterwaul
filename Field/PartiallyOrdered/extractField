let object = Type

let cat = ./../../Category/Set/monoidal/cartesian

in    λ(m : object)
    → λ(field : ./Type m)
    →   { additive =
            ./../../Group/PartiallyOrderedCommutative/extractCommutativeGroup
            m
            field.additive
        , multiplicative =
            field.multiplicative.{ identity, inverse, op }
        }
      : ./../Type object cat m
