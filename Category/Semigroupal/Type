let kCat = ./../Cat/semigroupal

let vObject = Type

in    λ(object : Kind)
    → λ(v : ./../Monoidal/Kind kCat Type vObject)
    → λ(cat : ./Kind kCat vObject object)
    → λ(m : object)
    → { category :
          ./../Type object v (./extractCategory kCat vObject object cat)
      , monoid :
          ./../../Semigroup/Type object cat m
      }
