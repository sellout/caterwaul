let Equality =
      ./../Eq/Type sha256:6084124c32c2894f3ef6aff7f8762769b309face3a6b507b3cd1108d2fa6b179

in    λ(a : Type)
    → λ(eq : Equality a)
    →   { eq =
              λ(x : Optional a)
            → λ(y : Optional a)
            → Optional/fold
              a
              x
              Bool
              (λ(xx : a) → Optional/fold a y Bool (eq.eq xx) False)
              (Optional/fold a y Bool (λ(yy : a) → False) True)
        }
      : Equality (Optional a)
