let kArrow = ./../Function/Kind

let kProduct = ./../Tuple/Kind

let vObject = Type

in    λ(object : Kind)
    → λ(cat : ./../Category/Monoidal/Kind kArrow kProduct vObject object)
    → λ(m : object)
    → λ(lattice : ./Type object cat m)
    → lattice.meet : ./../Monoid/Commutative/Type object cat m
