let object = Type

let arrow = ./../Function/Type

in    { unit =
          λ(a : object) → λ(x : a) → x
      , counit =
          λ(a : object) → λ(x : a) → x
      , leftAdjunct =
          λ(a : object) → λ(b : object) → λ(f : a → b) → f
      , rightAdjunct =
          λ(a : object) → λ(b : object) → λ(f : a → b) → f
      }
    : ./../Adjunction/Type
      object
      object
      arrow
      arrow
      (./Type object)
      (./Type object)
