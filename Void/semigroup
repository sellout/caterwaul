let kArrow = ./../Function/Kind

let kProduct = ./../Tuple/Kind

let object = Type

let cat =
      ./../Category/Monoidal/extractSemigroupal
      kArrow
      kProduct
      object
      ./../Category/Set/monoidal/cartesian

in    { op = λ(p : { _1 : <>, _2 : <> }) → merge {=} p._1 : <> }
    : ./../Semigroup/Type object cat <>
