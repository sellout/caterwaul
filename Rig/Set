let kCat = ./../Category/Cat/semigroupal

let vObject = Type

let object = Type

let cat =
      ./../Category/Monoidal/Cartesian/extractMonoidal
        kCat
        vObject
        object
        ./../Category/Set/monoidal/cartesian

in    λ(m : object)
    → { additive :
          ./../Monoid/Commutative/Set m
      , multiplicative :
          ./../Monoid/Type object cat m
      }
