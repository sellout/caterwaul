let kCat = ./../../Category/Cat/semigroupal

in    λ(vObject : Kind)
    → λ(object : Kind)
    → λ(cat : ./../Kind kCat vObject object)
    → λ(m : kCat.arrow object object)
    →   { constraint =
            cat.constraint
        , arrow =
            ./../../Star/Type vObject object cat m
        }
      : ./../Kind kCat vObject object
