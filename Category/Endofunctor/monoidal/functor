let kCat = ./../../Cat/semigroupal

let vObject = Type

let object = Type

in    λ(v : ./../../Kind kCat Type vObject)
    → λ(cat : ./../../Monoidal/Kind kCat vObject object)
    → ./../../Duoidal/extractDiamond
        kCat
        vObject
        (kCat.arrow object object)
        (./../duoidal v cat)
