  λ(arrow : Kind → Kind → Kind)
→ λ(product : Kind → Kind → Kind)
→ λ(object : Kind)
→ λ(rig : ./Kind arrow product object)
→   { constraint =
        rig.constraint
    , arrow =
        rig.arrow
    , identity =
        rig.multiplicative.identity
    , op =
        rig.multiplicative.op
    }
  : ./../Monoidal/Kind arrow product object
