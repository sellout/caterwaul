let kArrow = ./../../Function/Kind

let kProduct = ./../../Tuple/Kind

let Category = ./../../Category/Kind kArrow kProduct

let Functor = ./../Type

in    λ(vObject : Kind)
    → λ(object : Kind)
    → λ(v : Category Type vObject)
    → λ(cat : Category vObject object)
    → Functor vObject object object v cat cat
