let kArrow = ./../../Function/Kind

let kProduct = ./../../Tuple/Kind

in    λ(object : Kind)
    → λ(cat : ./../Semigroupal/Kind kArrow kProduct object)
    →   { constraint =
            cat.constraint
        , arrow =
            ./Type object cat.arrow
        , op =
            cat.op
        }
      : ./../Semigroupal/Kind kArrow kProduct object
