let kCat = ./../Category/Cat/semigroupal

let vObject = Type

in    λ(object : Kind)
    → λ(cat : ./../Category/Monoidal/Kind kCat vObject object)
    → λ(m : object)
    → λ(ring : ./Type object cat m)
    →   { additive =
            ./../Group/Commutative/extractCommutativeMonoid
              object
              cat
              m
              ring.additive
        , multiplicative =
            ring.multiplicative
        }
      : ./../Rig/Type object cat m
