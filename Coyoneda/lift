let kCat = ./../Category/Cat/semigroupal

let vObject = Type

let object = Type

in    λ(v : ./../Category/Monoidal/Kind kCat Type vObject)
    → λ(cat : ./../Category/Kind kCat vObject object)
    → λ(category : ./../Category/Type object v cat)
    → ./../Lan/lift
      v
      cat
      (./../Category/extractSemigroupoid object v cat category)
      (./../Identity/Type object)
      (./../Identity/comonad object v cat category)
