let cat = ./../Category/Cat/semigroupal

let vObject = Type

let Set =
      ./../Category/Monoidal/Cartesian/extractMonoidal
        cat
        vObject
        Type
        ./../Category/Set/monoidal/cartesian

in    { unit =
          λ(_ : {}) → ""
      , product =
          λ(a : { _1 : Text, _2 : Text }) → a._1 ++ a._2
      }
    : ./../Monoid/Type Type Set Text
