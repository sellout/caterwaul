  λ(cat : ./../Monoidal/Sort)
→ λ(vObject : Kind)
→ λ(object : Kind)
→ λ(duoidal : ./Kind cat vObject object)
→   { constraint = duoidal.constraint, arrow = duoidal.arrow }
  : ./../Kind cat vObject object
