let kArrow = ./../../Function/Kind

let kProduct = ./../../Tuple/Kind

let vObject = Type

in    λ(object : Kind)
    → λ(v : ./Kind kArrow kProduct Type vObject)
    → λ(cat : ./Kind kArrow kProduct vObject object)
    → λ(m : object)
    → { category :
          ./../Type
          object
          v
          (./extractCategory kArrow kProduct vObject object cat)
      , monoid :
          ./../../Monoid/Type object cat m
      }
