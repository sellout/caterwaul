      in    { identity =
                  λ(a : cObject)
                → { subst =
                        λ(f : cat.arrow cObject dObject)
                      → dCategory.identity (f a)
                  }
            , product =
                  λ(a : { _1 : cObject, _2 : cObject })
                → λ ( p
                    :   ∀(r : Type)
                      → (   ∀(z : cObject)
                          → Leibniz.arrow { _1 = z, _2 = a._2 }
                          → Leibniz.arrow { _1 = a._1, _2 = z }
                          → r
