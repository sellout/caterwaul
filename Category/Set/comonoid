let cat = ./../Cat/semigroupal

let vObject = Type

let Set =
      ./../Monoidal/Cartesian/extractMonoidal
        cat
        vObject
        Type
        ./monoidal/cartesian

in    λ(a : Type)
    →   { unit = λ(_ : a) → {=}, product = λ(x : a) → { _1 = x, _2 = x } }
      : ./../../Comonoid/Type Type Set a
