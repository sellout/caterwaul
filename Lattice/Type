let kCat = ./../Category/Cat/semigroupal

let vObject = Type

in    λ(object : Kind)
    → λ(cat : ./../Category/Monoidal/Kind kCat vObject object)
    → λ(m : object)
    → { join :
          ./../Monoid/Commutative/Type object cat m
      , meet :
          ./../Monoid/Commutative/Type object cat m
      }
