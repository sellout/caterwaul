let kArrow = ./../Function/Kind

let kProduct = ./../Tuple/Kind

let vObject = Type

in    λ(cObject : Kind)
    → let dObject = Type
      
      in    λ(d : ./../Category/Kind kArrow kProduct vObject dObject)
          → λ(f : kArrow cObject dObject)
          → ./../Lan/Type cObject dObject d f f
