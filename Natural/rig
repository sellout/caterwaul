{- TODO: Should be a Rig/Commutative -}
let object = Type

let cat = ./../Category/Set/monoidal/cartesian

in    { additive =
          { identity =
              λ(_ : {}) → 0
          , op =
              λ(t : { _1 : Natural, _2 : Natural }) → t._1 + t._2
          }
      , multiplicative =
          { identity =
              λ(_ : {}) → 1
          , op =
              λ(t : { _1 : Natural, _2 : Natural }) → t._1 * t._2
          }
      }
    : ./../Rig/Type object cat Natural
