let Equality =
      ./../Eq/Type sha256:16ec0250d1cd6f2d48bb5fe37d46d1db9ef640657c7b247b8d5be1aca30259b1

in    λ(a : Type)
    → λ(eq : Equality a)
    →   { eq =
              λ(x : Optional a)
            → λ(y : Optional a)
            → Optional/fold
              a
              x
              Bool
              (λ(xx : a) → Optional/fold a y Bool (eq.eq xx) False)
              (Optional/fold a y Bool (λ(yy : a) → False) True)
        }
      : Equality (Optional a)
