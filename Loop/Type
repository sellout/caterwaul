let kArrow = ./../Function/Kind

let kProduct = ./../Tuple/Kind

let vObject = Type

in    λ(object : Kind)
    → λ(cat : ./../Category/Monoidal/Kind kArrow kProduct vObject object)
    → λ(m : object)
    →   { identity : cat.arrow { _1 = cat.identity, _2 = m } }
      ⩓ ./../Quasigroup/Type
        object
        ( ./../Category/Monoidal/extractSemigroupal
          kArrow
          kProduct
          vObject
          object
          cat
        )
        m
