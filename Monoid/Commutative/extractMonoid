let cat = ./../../Category/Cat/semigroupal

let vObject = Type

let object = Type

let cat =
      ./../../Category/Monoidal/Cartesian/extractMonoidal
        cat
        vObject
        object
        ./../../Category/Set/monoidal/cartesian

in    λ(m : object)
    → λ(monoid : ./Set m)
    → monoid.{ unit, product } : ./../Type object cat m
