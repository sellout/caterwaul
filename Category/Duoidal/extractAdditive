  λ(kArrow : Kind → Kind → Kind)
→ λ(kProduct : Kind → Kind → Kind)
→ λ(vObject : Kind)
→ λ(object : Kind)
→ λ(duoidal : ./Kind kArrow kProduct vObject object)
→   { constraint =
        duoidal.constraint
    , arrow =
        duoidal.arrow
    , identity =
        duoidal.additive.identity
    , op =
        duoidal.additive.op
    }
  : ./../Monoidal/Kind kArrow kProduct vObject object
