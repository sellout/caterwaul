{- FIXME: Should be able to do `monoidal.{ arrow, product }`, but Dhall doesn’t like
          that for some reason.
-}
  λ(cat : ./Sort)
→ λ(vObject : Kind)
→ λ(object : Kind)
→ λ(monoidal : ./Kind cat vObject object)
→   { constraint =
        monoidal.constraint
    , arrow =
        monoidal.arrow
    , product =
        monoidal.product
    }
  : ./../Semigroupal/Kind cat vObject object
