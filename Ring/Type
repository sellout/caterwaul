let kArrow = ./../Function/Kind

let kProduct = ./../Tuple/Kind

let vObject = Type

in    λ(object : Kind)
    → λ(cat : ./../Category/Monoidal/Kind kArrow kProduct vObject object)
    → λ(m : object)
    → { additive :
          ./../Group/Commutative/Type object cat m
      , multiplicative :
          ./../Monoid/Type object cat m
      }
