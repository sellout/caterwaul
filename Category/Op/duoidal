let kArrow = ./../../Function/Kind

let kProduct = ./../../Tuple/Kind

in    λ(vObject : Kind)
    → λ(object : Kind)
    → λ(cat : ./../Duoidal/Kind kArrow kProduct vObject object)
    →     { additive = cat.additive, multiplicative = cat.multiplicative }
        ∧ ./Kind
          vObject
          object
          (./../Duoidal/extractCategory kArrow kProduct vObject object cat)
      : ./../Duoidal/Kind kArrow kProduct vObject object
