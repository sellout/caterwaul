let kCat = ./../../Category/Cat/semigroupal

let vObject = Type

in    λ(object : Kind)
    → λ(cat : ./../../Category/Monoidal/Kind kCat vObject object)
    → λ(m : object)
    → λ(rig : ./Type object cat m)
    →   { additive =
            ./../../Monoid/Commutative/Monus/extractCommutativeMonoid
              object
              cat
              m
              rig.additive
        , multiplicative =
            rig.multiplicative
        }
      : ./../Type object cat m
