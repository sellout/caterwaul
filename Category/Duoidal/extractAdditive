  λ(kArrow : Kind → Kind → Kind)
→ λ(kProduct : Kind → Kind → Kind)
→ λ(object : Kind)
→ λ(rig : ./Kind kArrow kProduct object)
→   { constraint =
        rig.constraint
    , arrow =
        rig.arrow
    , identity =
        rig.additive.identity
    , op =
        rig.additive.op
    }
  : ./../Monoidal/Kind kArrow kProduct object
