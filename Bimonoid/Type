let kArrow = ./../Function/Kind

let kProduct = ./../Tuple/Kind

in    λ(object : Kind)
    → λ(cat : ./../Category/Monoidal/Kind kArrow kProduct object)
    → λ(m : object)
    → { monoid :
          ./../Monoid/Type object cat m
      , comonoid :
          ./../Comonoid/Type object cat m
      }
