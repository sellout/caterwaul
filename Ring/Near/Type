let kCat = ./../../Category/Cat/semigroupal

let vObject = Type

in    λ(object : Kind)
    → λ(cat : ./../../Category/Monoidal/Kind kCat vObject object)
    → λ(m : object)
    → { additive :
          ./../../Group/Type object cat m
      , multiplicative :
          ./../../Monoid/Type object cat m
      }
