let object = Type

let cat = ./../../Category/Set/monoidal/cartesian

in    λ(m : object)
    → λ(ring : ./Set m)
    →   { additive =
            ring.additive.{ identity, inverse, op }
        , multiplicative =
            ./../../Monoid/Commutative/extractMonoid m ring.multiplicative
        }
      : ./../Type object cat m
