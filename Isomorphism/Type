let kCat = ./../Category/Cat/semigroupal

let vObject = Type

in    λ(object : Kind)
    → λ(cat : ./../Category/Kind kCat vObject object)
    →   (   λ(a : { _1 : object, _2 : object })
          → { to : cat.arrow a, from : cat.arrow { _1 = a._2, _2 = a._1 } }
        )
      : kCat.arrow (kCat.product object object) vObject
