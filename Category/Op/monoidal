let kCat = ./../Cat/semigroupal

in    λ(vObject : Kind)
    → λ(object : Kind)
    → λ(cat : ./../Monoidal/Kind kCat vObject object)
    →     { unit = cat.unit }
        ∧ ./semigroupal
          vObject
          object
          (./../Monoidal/extractSemigroupal kCat vObject object cat)
      : ./../Monoidal/Kind kCat vObject object
